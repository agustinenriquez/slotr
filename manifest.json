{
    "name": "Slotr",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-3.5.0.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Slotr"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}